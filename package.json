{
  "name": "nextjs-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:staging": "env-cmd -f .env.staging next dev",
    "build": "next build",
    "build:dev": "env-cmd -f .env.development next build",
    "build:staging": "env-cmd -f .env.staging next build",
    "build:prod": "env-cmd -f .env.production next build",
    "start": "next start -p 3002",
    "start:dev": "env-cmd -f .env.development next start -p 3002",
    "start:staging": "env-cmd -f .env.staging next start -p 3002",
    "start:prod": "env-cmd -f .env.production next start -p 3002",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "prepare": "husky install",
    "prettier:check": "prettier --config .prettierrc.js --check \"*.{ts,js,tsx,jsx,scss,css,html}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "yarn format",
      "yarn prettier:check",
      "eslint --fix",
      "git add ."
    ]
  },
  "dependencies": {
    "@binance-chain/bsc-connector": "^1.0.0",
    "@ethersproject/contracts": "^5.6.0",
    "@ethersproject/providers": "^5.5.3",
    "@google/model-viewer": "^1.10.1",
    "@reduxjs/toolkit": "^1.8.0",
    "@types/js-cookie": "^3.0.2",
    "@web3-react/core": "^6.1.9",
    "@web3-react/frame-connector": "^6.0.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/walletconnect-connector": "6.2.4",
    "@zeit/next-sass": "^1.0.1",
    "antd": "^4.18.9",
    "axios": "^0.26.1",
    "bignumber.js": "^9.0.2",
    "classnames": "^2.3.1",
    "ethers": "^5.6.2",
    "formik": "^2.2.9",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "next": "12.0.7",
    "next-compose-plugins": "^2.2.1",
    "next-i18next": "8.1.3",
    "next-images": "^1.8.4",
    "next-redux-wrapper": "^7.0.5",
    "next-seo": "^4.24.0",
    "react": "17.0.2",
    "react-countdown": "^2.3.3",
    "react-countdown-circle-timer": "^3.0.9",
    "react-dom": "17.0.2",
    "react-image-file-resizer": "^0.4.8",
    "react-infinite-scroll-component": "^6.1.0",
    "react-otp-input": "^3.0.2",
    "react-query": "^3.39.2",
    "react-redux": "^7.2.6",
    "react-share": "^4.4.0",
    "react-twitter-embed": "^4.0.4",
    "redux": "^4.1.2",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "sass": "^1.49.9",
    "socket.io-client": "^4.4.1",
    "swiper": "^8.4.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.179",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.14",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "8.10.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "prettier": "^2.2.1",
    "typescript": "^4.6.2"
  }
}
